{
  "compilerOptions": {
    "target": "es2018",
    "module": "esnext",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitThis": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom",
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@types/node",
      "@nuxt/types",
      "@nuxtjs/axios",
      "@types/nuxtjs__auth",
      "@nuxtjs/toast"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
